<launch>
	<node
		pkg="crash_the_husky"
		type="crash_the_husky_node"
		name="crash_the_husky"
	/>

	<node
		pkg="cartographer_ros"
		type="cartographer_node"
		name="cartographer"
		args="-configuration_directory $(env GCART_CONF_DIR) -configuration_basename husky_test.lua"
	/>
	
	<include file="$(find husky_gazebo)/launch/husky_empty_world.launch"/>

	<node
		pkg="rviz"
		type="rviz"
		name="rviz"
		args="-d $(find crash_the_husky)/resources/google_cart.rviz"
	/>

	<node
		pkg="rqt_robot_steering"
		type="rqt_robot_steering"
		name="rqt_robot_steering"
	/>

</launch>
